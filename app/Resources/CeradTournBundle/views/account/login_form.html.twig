<div class="account-signin">
<form action="{{ path("fos_user_security_check") }}" method="post">
    <label for="username">Zayso User Name or Email or 8-digit AYSOID</label>
    <input type="text" id="username" name="_username" value="{{ last_username }}" required="required" size="30" /><br />
    
    <label for="password">Password</label>
    <input type="password" id="password" name="_password" required="required" />
    
    <label for="remember_me">Remember Me</label>
    <input type="checkbox" id="remember_me" name="_remember_me" value="on" />
    
    <input type="submit" id="_submit" name="_submit" value="Sign In" />
    <input type="hidden" name="_csrf_token" value="{{ csrf_token }}" />
</form>
</div>